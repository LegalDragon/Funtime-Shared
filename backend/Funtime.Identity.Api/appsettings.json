{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "http://localhost:3000",
      "https://pickleball.community",
      "https://pickleball.college",
      "https://pickleball.date",
      "https://pickleball.jobs",
      "https://auth.funtime.com"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=FuntimeIdentity;Trusted_Connection=True;TrustServerCertificate=True;",
    "NotificationConnection": "Server=localhost;Database=fxEmail;Trusted_Connection=True;TrustServerCertificate=True;"
  },
    "Jwt": {
        "Key": "__JWT_SIGNING_KEY__",
        "Issuer": "FuntimePickleball",
        "Audience": "FuntimePickleballUsers",
        "ExpirationInMinutes": 60
    },
    "OAuth": {
        "Google": {
            "Active": false,
            "ClientId": "",
            "ClientSecret": ""
        },
        "Facebook": {
            "Active": false,
            "ClientId": "",
            "ClientSecret": ""
        },
        "Apple": {
            "Active": false,
            "ClientId": "",
            "TeamId": "",
            "KeyId": "",
            "PrivateKey": ""
        },
        "Microsoft": {
            "Active": false,
            "ClientId": "",
            "ClientSecret": ""
        }
    },
    "Stripe": {
        "SecretKey": "__STRIPE_SECRET_KEY__",
        "PublishableKey": "__STRIPE_PUBLISHABLE_KEY__",
        "WebhookSecret": "__STRIPE_WEBHOOK_SECRET__"
    },
  "Twilio": {
    "AccountSid": "YOUR_TWILIO_ACCOUNT_SID",
    "AuthToken": "YOUR_TWILIO_AUTH_TOKEN",
    "PhoneNumber": "+1234567890"
  },
  "RateLimiting": {
    "OtpMaxAttempts": 5,
    "OtpWindowMinutes": 15
  },
  "Storage": {
    "Type": "local",
    "LocalPath": "f:\\docvault\\FuntimePickleball",
    "LocalBaseUrl": "",
    "OrganizeByMonth": true
  },
  "AWS": {
    "AccessKey": "YOUR_AWS_ACCESS_KEY",
    "SecretKey": "YOUR_AWS_SECRET_KEY",
    "Region": "us-east-1",
    "BucketName": "funtime-identity"
  },
  "ApiSecretKey": "__API_SECRET_KEY__",
  "PushNotifications": {
    "ApiKey": ""
  },
  "FXNotification": {
    "BaseUrl": "http://localhost:5100",
    "ApiKey": ""
  }
}
